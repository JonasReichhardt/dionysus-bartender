{"ast":null,"code":"export default {\n  name: 'sliders_area',\n\n  data() {\n    return {\n      pumpvals: [20, 20, 15, 15, 15, 15],\n      pumpvalscopy: [20, 20, 15, 15, 15, 15],\n      enablepump: [true, true, true, true, true, true],\n      counter: 0 //used for remainder handling in pumpvals\n\n    };\n  },\n\n  methods: {\n    normalizeVals: function (index) {\n      //add to every value (active and not index) the integer div of the difference and nActivePumps-1, if remainder than add 1 to pumpval[counter%pumpval.length] while counter%length != 0, so every pump increases same\n      console.log(\"Before values of og pumpvals: \" + this.pumpvals[0] + \" \" + this.pumpvals[1] + \" \" + this.pumpvals[2] + \" \" + this.pumpvals[3] + \" \" + this.pumpvals[4] + \" \" + this.pumpvals[5]);\n      let absDiff = this.pumpvals[index] - this.pumpvalscopy[index];\n      let currPump = index;\n      let nActivePumps = 0;\n\n      for (let i in this.enablepump) {\n        //compute num of active pumps\n        if (this.enablepump[i]) {\n          nActivePumps++;\n        }\n      }\n\n      let DiffPerPump = -1 * (absDiff < 0 ? Math.ceil(absDiff / (nActivePumps - 1)) : Math.floor(absDiff / (nActivePumps - 1)));\n      let remainder = absDiff % (nActivePumps - 1);\n      let ogremainder = remainder;\n\n      for (var i = 0; i < this.pumpvals.length; i++) {\n        if (this.enablepump[i] && i != index) {\n          this.pumpvals[i] += DiffPerPump;\n        }\n      } //TODO normalize the vals of all (active) remaining pumps\n\n\n      while (remainder != 0) {\n        if (this.counter % this.pumpvals.length != index) {\n          if (remainder > 0) {\n            this.pumpvals[this.counter % this.pumpvals.length]--;\n            remainder--;\n          } else {\n            this.pumpvals[this.counter % this.pumpvals.length]++;\n            remainder++;\n          }\n        }\n\n        this.counter++;\n      }\n\n      for (let i = 0; i < this.pumpvals.length; i++) {\n        //update pumpvalscopy\n        this.pumpvalscopy[i] = this.pumpvals[i];\n      }\n\n      var sum = 0;\n\n      for (let i = 0; i < this.pumpvals.length; i++) {\n        sum += this.pumpvals[i] * 1;\n      }\n\n      console.log(\"absDiff: \" + absDiff + \"\\ncurrPump: \" + currPump + \"\\nnActivePumps: \" + nActivePumps + \"\\nDiffPerPump: \" + DiffPerPump + \" \\nRemainder: \" + ogremainder + \"\\nsum: \" + sum);\n      console.log(\"After the function: \" + this.pumpvalscopy[0] + \" \" + this.pumpvalscopy[1] + \" \" + this.pumpvalscopy[2] + \" \" + this.pumpvalscopy[3] + \" \" + this.pumpvalscopy[4] + \" \" + this.pumpvalscopy[5]);\n    }\n  }\n};","map":{"version":3,"mappings":"AA0CA,eAAc;AACVA,MAAI,EAAE,cADI;;AAEVC,MAAI,GAAE;AACJ,WAAM;AACJC,cAAQ,EAAE,CAAC,EAAD,EAAI,EAAJ,EAAO,EAAP,EAAU,EAAV,EAAa,EAAb,EAAgB,EAAhB,CADN;AAEJC,kBAAY,EAAE,CAAC,EAAD,EAAK,EAAL,EAAS,EAAT,EAAa,EAAb,EAAiB,EAAjB,EAAqB,EAArB,CAFV;AAGJC,gBAAU,EAAE,CAAC,IAAD,EAAO,IAAP,EAAa,IAAb,EAAmB,IAAnB,EAAyB,IAAzB,EAA+B,IAA/B,CAHR;AAIJC,aAAO,EAAE,CAJL,CAIO;;AAJP,KAAN;AAMD,GATS;;AAUVC,SAAO,EAAC;AACJC,iBAAa,EAAE,UAASC,KAAT,EAAe;AAAE;AAChCC,aAAO,CAACC,GAAR,CAAY,mCAAmC,KAAKR,QAAL,CAAc,CAAd,CAAnC,GAAsD,GAAtD,GAA4D,KAAKA,QAAL,CAAc,CAAd,CAA5D,GAA+E,GAA/E,GAAqF,KAAKA,QAAL,CAAc,CAAd,CAArF,GAAwG,GAAxG,GAA8G,KAAKA,QAAL,CAAc,CAAd,CAA9G,GAAiI,GAAjI,GAAuI,KAAKA,QAAL,CAAc,CAAd,CAAvI,GAA0J,GAA1J,GAAgK,KAAKA,QAAL,CAAc,CAAd,CAA5K;AACE,UAAIS,OAAM,GAAI,KAAKT,QAAL,CAAcM,KAAd,IAAuB,KAAKL,YAAL,CAAkBK,KAAlB,CAArC;AACA,UAAII,QAAO,GAAIJ,KAAf;AAEA,UAAIK,YAAW,GAAI,CAAnB;;AACA,WAAI,IAAIC,CAAR,IAAa,KAAKV,UAAlB,EAA6B;AAAE;AAC7B,YAAG,KAAKA,UAAL,CAAgBU,CAAhB,CAAH,EAAsB;AACpBD,sBAAY;AACd;AACF;;AACA,UAAIE,WAAU,GAAK,CAAC,CAAF,IAAMJ,OAAM,GAAI,CAAV,GAAcK,IAAI,CAACC,IAAL,CAAUN,OAAO,IAAEE,YAAY,GAAC,CAAf,CAAjB,CAAd,GAAoDG,IAAI,CAACE,KAAL,CAAWP,OAAO,IAAEE,YAAY,GAAC,CAAf,CAAlB,CAA1D,CAAlB;AACA,UAAIM,SAAQ,GAAIR,OAAO,IAAEE,YAAY,GAAC,CAAf,CAAvB;AACA,UAAIO,WAAU,GAAID,SAAlB;;AAEA,WAAI,IAAIL,IAAI,CAAZ,EAAeA,IAAI,KAAKZ,QAAL,CAAcmB,MAAjC,EAAyCP,CAAC,EAA1