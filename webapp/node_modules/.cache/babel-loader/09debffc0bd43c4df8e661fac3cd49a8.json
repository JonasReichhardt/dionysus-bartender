{"ast":null,"code":"export default {\n  name: 'sliders_area',\n\n  data() {\n    return {\n      pumpvals: [20, 20, 15, 15, 15, 15],\n      pumpvalscopy: [20, 20, 15, 15, 15, 15],\n      enablepump: [true, true, true, true, true, true],\n      counter: 0 //used for remainder handling in pumpvals\n\n    };\n  },\n\n  methods: {\n    normalizeVals: function (index) {\n      //add to every value (active and not index) the integer div of the difference and nActivePumps-1, if remainder than add 1 to pumpval[counter%pumpval.length] while counter%length != 0, so every pump increases same\n      let absDiff = this.pumpvals[index] - this.pumpvalscopy[index];\n      let currPump = index;\n      let nActivePumps = 0;\n\n      for (let i in this.enablepump) {\n        //compute num of active pumps\n        if (this.enablepump[i]) {\n          nActivePumps++;\n        }\n      }\n\n      let DiffPerPump = -1 * Math.floor(absDiff / (nActivePumps - 1));\n      let remainder = absDiff % (nActivePumps - 1);\n\n      for (var i = 0; i < this.pumpvals.length; i++) {\n        if (this.enablepump[i] && i != index) {\n          this.pumpvals[i] += DiffPerPump;\n        }\n      } //TODO normalize the vals of all (active) remaining pumps\n\n\n      for (let i = 0; i < this.pumpvals.length; i++) {\n        //update pumpvalscopy\n        this.pumpvalscopy[i] = this.pumpvals[i];\n      }\n\n      var sum = 0;\n\n      for (let i = 0; i < this.pumpvals.length; i++) {\n        sum += this.pumpvals[i] * 1;\n      }\n\n      sum += remainder;\n      console.log(\"absDiff: \" + absDiff + \"\\ncurrPump: \" + currPump + \"\\nnActivePumps: \" + nActivePumps + \"\\nDiffPerPump: \" + DiffPerPump + \" \\nRemainder: \" + remainder + \"\\nsum: \" + sum);\n    }\n  }\n};","map":{"version":3,"mappings":"AA0CA,eAAc;AACVA,MAAI,EAAE,cADI;;AAEVC,MAAI,GAAE;AACJ,WAAM;AACJC,cAAQ,EAAE,CAAC,EAAD,EAAI,EAAJ,EAAO,EAAP,EAAU,EAAV,EAAa,EAAb,EAAgB,EAAhB,CADN;AAEJC,kBAAY,EAAE,CAAC,EAAD,EAAK,EAAL,EAAS,EAAT,EAAa,EAAb,EAAiB,E